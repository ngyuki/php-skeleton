{
    "name": "ngyuki/php-skeleton",
    "description": "php-skeleton",
    "type": "library",
    "keywords": [ "skeleton" ],
    "homepage": "https://github.com/ngyuki/php-skeleton",
    "license": "MIT",
    "authors": [
        { "name": "ngyuki", "email": "ngyuki.ts@gmail.com" }
    ],
    "autoload": {
        "psr-4": {
            "ngyuki\\Skeleton\\": "src/"
        }
    },
    "repositories": [
        { "packagist": false }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-pcre": "*"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "bin": [
        "bin/ngyuki-php-skeleton"
    ]
