{
    "name": "ngyuki/php-skeleton",
    "description": "php skeleton",
    "type": "library",
    "keywords": [ "skeleton" ],
    "homepage": "https://github.com/ngyuki/php-skeleton",
    "license": "MIT",
    "authors": [
        { "name": "ngyuki", "email": "ngyuki.jp@gmail.com" }
    ],
    "autoload": {
        "psr-4": {
            "ngyuki\\Skeleton\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "5.4.16"
        }
    },
    "require": {
        "php": ">=5.4.16",
        "ext-pcre": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "satooshi/php-coveralls": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "bin": [
        "bin/skeleton"
    ]
}
